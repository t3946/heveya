<div
    class="pieChart pieChart_size_110"
    data-inner-label="{{label}}"
    data-inner-label-color="{{labelColor}}"
    data-inner-label-caption="{{labelCaption}}"
    data-inner-label-center-y="20"
    data-inner-radius="80"
    data-inner-label-caption-center-y="-5"
    data-colors='{{colors}}'
    data-values='{{values}}'
    data-categories='["A", "B", "C"]'
    data-font-size="1.8"
></div>