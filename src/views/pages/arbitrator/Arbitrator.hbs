{{> ../../layout/Hat}}

<div class="arbitratorPage page">
    <div class="container-lg">
        {{> ../../common/breadcrumbs/Breadcrumbs }}

        <h1 class="header1 page__block">Арбитражный управляющий Константинов Константин Константинович</h1>


        <div class="container-lg containerFrame page__block">
            {{> ../../common/personal-data-panel/arbitrator/Arbitrator }}
        </div>

        <div class="containerFrame d-flex justify-content-between page__block">
            <div class="nameValuePair">
                <div class="name">Средняя сумма договора:</div>
                <div class="value">6 000 ₽</div>
            </div>

            <div class="nameValuePair">
                <div class="name">Максимальная сумма сделки:</div>
                <div class="value">50 000 000 000 ₽</div>
            </div>

            <div class="nameValuePair">
                <div class="name">Количество площадок:</div>
                <div class="value">22</div>
            </div>

            <div class="nameValuePair">
                <div class="name">Выступал организатором:</div>
                <div class="value">56</div>
            </div>

            <div class="nameValuePair">
                <div class="name">Привлекал организаторов:</div>
                <div class="value">17</div>
            </div>
        </div>

        <div class="containerFrame page__block">
            <div class="row">
                <div class="col-6">
                    <div class="debtorsChart" style="width: 800px;height: 370px"></div>
                </div>
                <div class="col-6">
                    <div class="averagePriceChart" style="width: 800px;height: 370px"></div>
                </div>
            </div>
        </div>

        <div class="containerFrame page__block">
            <h2 class="header2 mb-4 text-center">Аукцион</h2>

            <div class="row">
                <div class="col-6">
                    <div class="auctionBlockChart1" style="width: 800px;height: 370px"></div>
                </div>

                <div class="col-6">
                    <div class="auctionBlockChart2" style="width: 800px;height: 370px"></div>
                </div>
            </div>

            <div class="row">
                <div class="col-6">
                    <div class="auctionBlockChart3" style="width: 800px;height: 370px"></div>
                </div>

                <div class="col-6">
                    <div class="auctionBlockChart4" style="width: 800px;height: 370px"></div>
                </div>
            </div>
        </div>

        <div class="containerFrame page__block">
            <h2 class="header2 mb-4 text-center">Публичное предложение</h2>

            <div class="row">
                <div class="col-6">
                    <div class="auctionBlockChart1" style="width: 800px;height: 370px"></div>
                </div>

                <div class="col-6">
                    <div class="auctionBlockChart2" style="width: 800px;height: 370px"></div>
                </div>
            </div>

            <div class="row">
                <div class="col-6">
                    <div class="auctionBlockChart3" style="width: 800px;height: 370px"></div>
                </div>

                <div class="col-6">
                    <div class="auctionBlockChart4" style="width: 800px;height: 370px"></div>
                </div>
            </div>
        </div>

        <div class="containerFrame page__block">
            <h2 class="header2 mb-4 text-center arbitratorPage__mapHeader1">География должников</h2>

            <div class="arbitratorPage__map1">{{> ../../common/top10/Top10}}</div>

            <h2 class="header2 mb-4 text-center arbitratorPage__mapHeader2">География участников торгов</h2>

            <div class="arbitratorPage__map2">{{> ../../common/top10/Top10}}</div>
        </div>

        <div class="containerFrame page__block">
            <h2 class="header2 mb-4 text-center arbitratorPage__mapHeader1">Нарушения</h2>

            <div class="d-flex justify-content-between">
                <div class="nameValuePair">
                    <div class="name">Должник:</div>
                    <div class="value">ООО Ромашка</div>
                </div>

                <div class="nameValuePair">
                    <div class="name">Дата:</div>
                    <div class="value">17.05.22</div>
                </div>

                <div class="nameValuePair">
                    <div class="name">№ дела:</div>
                    <div class="value">А27-365</div>
                </div>

                <div class="nameValuePair">
                    <div class="name">Решение:</div>
                    <div class="value">Штраф</div>
                </div>
            </div>
        </div>


        <div class="mb-4">
            <h2 class="header2">Оценка пользователей</h2>

            <div class="row">
                <div class="col-6">
                    <div class="grayPanel ratingPanel">
                        <div class="fs20px fw-semibold ratingHeader">Удобный интерфейс</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 67%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Понравился поиск лотов</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 88%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Техническая поддержка на связи</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 48%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Техническая поддержка помогает</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 67%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Простая регистрация</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 75%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Аккредитация в среднем занимает:</div>

                        <div class="groupRatingProgress">
                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">1 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">2 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">3 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">4 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">5 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">5-10 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">10 дней и более</span>
                            </div>
                        </div>
                    </div>

                    <div class="baseHorizontalDivider arbitratorPage__ratePanelsDivider"></div>

                    <div class="grayPanel ratingPanel">
                        <div class="fs20px fw-semibold ratingHeader">Необходимость оплачивать OID</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 67%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Взимают плату за  «ускоренную аккредитацию»</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 88%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Дополнительные комиссии с участников</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 48%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                    </div>
                    
                    <div class="d-flex arbitratorPage__addOpinionButton">
                        <div class="baseButton baseButton_theme_blue baseButton_size_medium">Добавить мнение</div>
                    </div>
                </div>

                <div class="col-6">
                    <div class="grayPanel ratingPanel">
                        <div class="fs20px fw-semibold ratingHeader">Быстродействие площадки</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 67%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Надежность работы площадки</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 67%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Сложность подачи заявки</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 67%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>

                        <div class="fs20px fw-semibold ratingHeader">Технические сложности при подготовке к работе на площадке </div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 67%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                    </div>

                    <div class="baseHorizontalDivider arbitratorPage__ratePanelsDivider"></div>

                    <div class="grayPanel ratingPanel">
                        <div class="fs20px fw-semibold ratingHeader">Сроки возврата задатка:</div>

                        <div class="groupRatingProgress">
                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">Не принимают задаток</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">На 1 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">На 2 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">На 3 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">На 4  день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">На 5 день</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">Более 5 дней</span>
                            </div>

                            <div class="progress baseProgress statProgress">
                                <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                     aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                <span class="progressComment">Не вернули</span>
                            </div>
                        </div>
                    </div>

                    <div class="baseHorizontalDivider arbitratorPage__ratePanelsDivider"></div>

                    <div class="grayPanel ratingPanel">
                        <div class="fs20px fw-semibold ratingHeader">Доверие к площадке</div>
                        <div class="progress baseProgress statProgress">
                            <div class="progress-bar" role="progressbar" data-label="25%" style="width: 25%"
                                 aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <h2 class="arbitratorPage__lotsList header2 text-center">
            Список лотов
        </h2>

        <div class="row">
            <div class="col-12">
                {{> ../../common/forms/SearchForm}}
            </div>
        </div>

        <div class="arbitratorPage__lotItem">
            {{> ../../common/lot-card/line/Completed }}
        </div>

        <div class="arbitratorPage__lotItem">
            {{> ../../common/lot-card/line/Completed }}
        </div>

        <div class="arbitratorPage__lotItem">
            {{> ../../common/lot-card/line/Completed }}
        </div>

        <div class="arbitratorPage__lotItem">
            {{> ../../common/lot-card/line/Completed }}
        </div>

        <div class="d-flex justify-content-center mt-4">
            {{> ../../common/pagination/Pagination }}
        </div>
    </div>
</div>

{{> ../../layout/Footer}}
